<datapower-configuration version="3">
    <configuration domain="default">
        <StylePolicyAction name="slm-shape" xmlns:env="http://www.w3.org/2003/05/soap-envelope" xmlns:dp="http://www.datapower.com/schemas/management">
            <mAdminState>enabled</mAdminState>
            <Type>slm</Type>
            <Input>INPUT</Input>
            <ParseSettingsReference>
                <URL/>
                <Literal/>
                <Default/></ParseSettingsReference>
            <ParseMetricsResultType>none</ParseMetricsResultType>
            <TransformLanguage>none</TransformLanguage>
            <ActionDebug persisted="false">off</ActionDebug>
            <TxMode>default</TxMode>
            <NamedInOutLocationType>default</NamedInOutLocationType>
            <SSLClientConfigType>proxy</SSLClientConfigType>
            <OutputType>default</OutputType>
            <Transactional>off</Transactional>
            <SLMPolicy>slm-shape</SLMPolicy>
            <SOAPValidation>body</SOAPValidation>
            <SQLSourceType>static</SQLSourceType>
            <JOSESerializationType>compact</JOSESerializationType>
            <JWEEncAlgorithm>A128CBC-HS256</JWEEncAlgorithm>
            <JOSEVerifyType>identifiers</JOSEVerifyType>
            <JOSEDecryptType>identifiers</JOSEDecryptType>
            <JWSVerifyStripSignature>on</JWSVerifyStripSignature>
            <Asynchronous>off</Asynchronous>
            <ResultsMode>first-available</ResultsMode>
            <RetryCount>0</RetryCount>
            <RetryInterval>1000</RetryInterval>
            <MultipleOutputs>off</MultipleOutputs>
            <IteratorType>XPATH</IteratorType>
            <Timeout>0</Timeout>
            <MethodRewriteType>GET</MethodRewriteType>
            <MethodType>POST</MethodType>
            <MethodType2>POST</MethodType2>
        </StylePolicyAction>
        <StylePolicyRule name="slm-main" xmlns:env="http://www.w3.org/2003/05/soap-envelope" xmlns:dp="http://www.datapower.com/schemas/management">
            <mAdminState>enabled</mAdminState>
            <Direction>rule</Direction>
            <InputFormat>none</InputFormat>
            <OutputFormat>none</OutputFormat>
            <NonXMLProcessing>on</NonXMLProcessing>
            <Unprocessed>off</Unprocessed>
            <Actions class="StylePolicyAction">slm-shape</Actions>
        </StylePolicyRule>
        <SLMPolicy name="slm-shape" xmlns:env="http://www.w3.org/2003/05/soap-envelope" xmlns:dp="http://www.datapower.com/schemas/management">
            <mAdminState>enabled</mAdminState>
            <ExecutionPolicy>execute-all-statements</ExecutionPolicy>
            <Statement>
                <SLMId>2</SLMId>
                <UserString/>
                <CredClass/>
                <RsrcClass/>
                <Schedule/>
                <Action class="SLMAction">shape</Action>
                <ThreshIntervalLength>0</ThreshIntervalLength>
                <ThreshIntervalType>concurrent</ThreshIntervalType>
                <ThreshAlgorithm>greater-than</ThreshAlgorithm>
                <ThresholdType>count-all</ThresholdType>
                <ThresholdLevel>10</ThresholdLevel>
                <ReleaseThresholdLevel>0</ReleaseThresholdLevel>
                <BurstLimit>0</BurstLimit>
                <ReportingAggregationInterval>0</ReportingAggregationInterval>
                <MaximumTotalReportingRecords>5000</MaximumTotalReportingRecords>
                <AutoGeneratedByWebGUI>off</AutoGeneratedByWebGUI>
                <MaximumResourcesAndCredentialsForThreshold>5000</MaximumResourcesAndCredentialsForThreshold>
            </Statement>
            <PeerGroup class="PeerGroup">webapi-peering</PeerGroup>
            <APIMgmt>on</APIMgmt>
        </SLMPolicy>
    </configuration>
    <files/>
</datapower-configuration>
